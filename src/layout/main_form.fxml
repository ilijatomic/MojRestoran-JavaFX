<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="main_bPane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layout.controller.MainForm">
    <top>
        <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
                <Button mnemonicParsing="false" onAction="#logout" text="Izloguj se" />
                <Button mnemonicParsing="false" onAction="#showProfil" text="Profil" />
            </items>
        </ToolBar>
    </top>
    <left>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <ListView fx:id="main_list" />
            </children>
        </VBox>
    </left>
</BorderPane>
